{
  "name": "villagers",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "g-map": "npx ts-node ./dist/src/static-data/generate-map.js",
    "g-schema": "npx openapi-typescript ./src/open-api/schema.yaml -o ./src/open-api/schema.ts && yarn g-enum",
    "g-enum": "npx ts-node ./src/open-api/add-enums-to-schema.ts && eslint --fix \"./src/open-api/schema.ts\"",
    "p-generate": "npx prisma generate",
    "p-migrate": "npx prisma migrate dev",
    "p-seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.13.0",
    "@supabase/supabase-js": "^2.43.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/testing": "^10.3.9",
    "@nestjs/websockets": "^10.3.8",
    "@types/bcryptjs": "^2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "prisma": "^5.13.0",
    "source-map-support": "^0.5.21",
    "supabase": ">=1.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.4.4",
    "yaml": "^2.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
